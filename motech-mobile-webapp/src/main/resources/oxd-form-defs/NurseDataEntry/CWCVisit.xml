      <xf:item id="2">
        <xf:label>Penta 2</xf:label>
        <xf:value>2</xf:value>
      </xf:item>
      <xf:item id="3">
        <xf:label>Penta 3</xf:label>
        <xf:value>3</xf:value>
      </xf:item>
    </xf:select1>
    <xf:select1 bind="yellowfever">
