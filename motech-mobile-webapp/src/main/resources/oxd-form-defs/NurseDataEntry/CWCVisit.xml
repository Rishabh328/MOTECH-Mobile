    </xf:input>
    <xf:input bind="muac">
      <xf:label>MUAC (cm)</xf:label>
    </xf:input>
    <xf:input bind="height">
      <xf:label>Height (cm)</xf:label>
    </xf:input>
    <xf:select1 bind="maleInvolved">
