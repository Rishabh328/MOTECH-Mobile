<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.dreamoval.motech.core.DataAccess.Domain.ResponseDetails">
    <id name="responseId">
      <generator class="identity"/>
    </id>
    <property name="gatewayMessageId"/>
    <property name="recipientNumber"/>
    <property name="messageStatus"/>
    <many-to-one class="com.dreamoval.motech.core.DataAccess.Domain.MessageDetails" name="messageId"/>
    <set name="transitions">
      <key column="responseId"/>
      <one-to-many class="com.dreamoval.motech.core.DataAccess.Domain.Transition"/>
    </set>
  </class>
</hibernate-mapping>
