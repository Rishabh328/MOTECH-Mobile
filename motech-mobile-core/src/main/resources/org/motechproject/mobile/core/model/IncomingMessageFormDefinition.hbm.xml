<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.motechproject.mobile.core.model">
    <typedef class="org.motechproject.mobile.core.util.StringEnumUserType" name="Duplicatable">
        <param name="enumClassname">org.motechproject.mobile.core.model.Duplicatable</param>
    </typedef>
    <typedef class="org.motechproject.mobile.core.util.StringEnumUserType" name="incMessageFormDefinitionType">
        <param name="enumClassname">org.motechproject.mobile.core.model.IncMessageFormDefinitionType</param>
    </typedef>
    <class name="IncomingMessageFormDefinitionImpl" table="incoming_message_form_definition">
        <id column="id" name="id" type="long">
            <generator class="assigned"/>
        </id>
        <version name="version">
            <column default="0" name="obj_vesion" sql-type="int"/>
        </version>
        <property column="form_code" length="100" name="formCode" type="string" unique="true"/>
        <property column="type" name="type" type="incMessageFormDefinitionType"/>
        <property column="duplicatable" name="duplicatable" type="Duplicatable"/>
        <property column="date_created" name="dateCreated" type="timestamp"/>
        <property column="send_response" name="sendResponse" type="boolean"/>
        <property generated="always" insert="false" name="lastModified" update="false">
            <column default="current_timestamp" name="last_modified" sql-type="timestamp"/>
        </property>
        <set cascade="save-update" name="incomingMsgParamDefinitions">
            <key column="incoming_message_form_definition_id"/>
            <one-to-many class="IncomingMessageFormParameterDefinitionImpl"/>
        </set>
        <set name="incomingMessageForms">
            <key column="incoming_message_form_definition_id"/>
            <one-to-many class="IncomingMessageFormImpl"/>
        </set>
    </class>
</hibernate-mapping>
