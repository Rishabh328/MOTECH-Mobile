<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.motechproject.mobile.core.model">
    <typedef class="org.motechproject.mobile.core.util.StringEnumUserType" name="incMessageFormStatus">
    <param name="enumClassname">org.motechproject.mobile.core.model.IncMessageFormStatus</param>
  </typedef>
  <class name="IncomingMessageFormImpl" table="incoming_message_form">
    <id column="id" name="id" type="long">
      <generator class="assigned"/>
    </id>
    <version name="version">
      <column default="0" name="obj_vesion" sql-type="int"/>
    </version>
    <many-to-one class="IncomingMessageFormDefinitionImpl" column="incoming_message_form_definition_id" name="incomingMsgFormDefinition"/>
    <property column="date_created" name="dateCreated" type="timestamp"/>
    <property generated="always" insert="false" name="lastModified" update="false">
      <column default="current_timestamp" name="last_modified" sql-type="timestamp"/>
    </property>
    <map cascade="save-update delete-orphan" inverse="true" name="incomingMsgFormParameters" table="inc_message_form_parameter">
      <key column="incoming_message_form_id"/>
      <map-key column="name" type="string"/>
      <one-to-many class="IncomingMessageFormParameterImpl"/>
    </map>
    <property access="field" column="message_form_status" name="messageFormStatus" type="incMessageFormStatus"/>
  </class>
</hibernate-mapping>
