<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dreamoval.motech.core.model">
  <class name="LanguageImpl" table="language">
    <id name="id">
      <generator class="assigned"/>
    </id>
    <property column="code" length="10" name="code" type="string" unique="true"/>
    <property column="name" length="50" name="name" type="string"/>
    <property column="description" name="description" type="string"/>
  </class>
</hibernate-mapping>
