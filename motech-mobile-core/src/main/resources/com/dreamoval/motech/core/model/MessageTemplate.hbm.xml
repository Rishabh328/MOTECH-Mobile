<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dreamoval.motech.core.model">
  <union-subclass  name="MessageTemplateImpl" extends="MotechEntityImpl">
      <!--<property name="language" length="10" />-->
      <property name="dateCreated" type="date" />
      <property name="messageType" type="messageType" update="false" access="field" />
      <property name="template" />
      <many-to-one name="notificationType" column="notification_type" class="NotificationTypeImpl" />
      <many-to-one name="language" column="language" class="LanguageImpl" />
  </union-subclass>
</hibernate-mapping>
