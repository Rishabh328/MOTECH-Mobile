<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dreamoval.motech.model.imp">
  <union-subclass extends="com.dreamoval.motech.core.model.MotechEntityImpl" name="IncomingMessageResponseImpl" table="incoming_message_response">
    <one-to-one class="IncomingMessageImpl" name="incomingMessage" property-ref="incomingMessageResponse"/>
    <property column="response_content" length="700" name="content" type="string"/>
    <property column="date_created" name="dateCreated" type="timestamp"/>
    <property generated="always" insert="false" name="lastModified" type="timestamp" update="false">
      <column default="current_timestamp" name="last_modified"/>
    </property>
    <property access="field" column="message_status" name="messageResponseStatus" type="incMessageResponseStatus"/>
  </union-subclass>
</hibernate-mapping>
