<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dreamoval.motech.model.imp">
    <union-subclass extends="com.dreamoval.motech.core.model.MotechEntityImpl" name="IncomingMessageImpl" table="incoming_message">
         <many-to-one class="IncomingMessageSessionImpl" column="incoming_message_session_id" name="incomingMsgSession"/>
        <property column="message_content" name="content" type="string" length="700"/>
        <property column="date_created" name="dateCreated" type="timestamp"/>
        <property name="lastModified" type="timestamp" generated="always" update="false" insert="false">
            <column name="last_modified" default="current_timestamp"/>
        </property>
        <property access="field" column="message_status" name="messageStatus" type="incMessageStatus" />
    </union-subclass>
</hibernate-mapping>
