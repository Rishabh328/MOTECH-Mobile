  <xf:model>
    <xf:instance id="child">
      <child id="77" name="CWC Visit -v1">
        <chpsid/>
        <facilityid/>
        <date/>
        <motechid/>
        <immunization/>
        <bcg/>
        <opvdose/>
        <pentadose/>
        <measles/>
        <yellowfever/>
